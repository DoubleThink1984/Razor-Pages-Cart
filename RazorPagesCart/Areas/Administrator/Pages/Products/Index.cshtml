@page
@model RazorPagesCart.Areas.Administrator.Pages.Products.IndexModel
@{
}
<h2>Products</h2>
<div class="row">
    <div class="col-md-12">
        <p>This is something</p>
        <p>
            <a asp-page="./Create">Create Something</a>
        </p>
        <hr />
        <ul>
            @foreach (var product in Model.AllProducts)
            {
                <li>
                    <a asp-page="./Edit" asp-route-id="@product.ProductID">@product.ProductName</a>
                </li>
            }
        </ul>
    </div>
</div>