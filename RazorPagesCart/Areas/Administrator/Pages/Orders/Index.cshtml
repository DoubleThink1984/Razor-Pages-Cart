@page "/SomeRoute/{somethingElse?}"
@model RazorPagesCart.Areas.Administrator.Pages.Orders.IndexModel
@{
}

<h2>Products</h2>
<div class="row">
    <div class="col-md-12">
        <p>This is something</p>
        <p>
            <a asp-page="/Create">Create Something</a>
        </p>
        <hr />
        <ul>
            <li>
                <a asp-page="../Products/Edit" asp-route-id="1">asdf - @ViewData["SomeData"]</a>
            </li>
            @foreach (Order order in Model.orders)
            {
                <li>
                    <a asp-page="./Edit" asp-route-id="@order.OrderID">@order.OrderDate</a>
                </li>
            }
        </ul>
    </div>
</div>
